<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ustb.entity.Teacher" table="t_teacher">
        <id name="tecNum" type="java.lang.String">
            <column name="TecNum" />
            <generator class="assigned" />
        </id>
        <property name="tecPass" type="java.lang.String">
            <column name="TecPass" />
        </property>
    </class>
</hibernate-mapping>
