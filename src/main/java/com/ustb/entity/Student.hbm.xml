<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.ustb.entity.Student" table="t_student">
		<id name="stuNum" type="java.lang.String">
			<column name="StuNum" />
			<generator class="assigned" />
		</id>
		<property name="stuName" type="java.lang.String">
			<column name="StuName" />
		</property>
		<property name="stuPass" type="java.lang.String">
			<column name="StuPass" />
		</property>
		<property name="stuGroup" type="java.lang.String">
			<column name="StuGroup" />
		</property>
	</class>
</hibernate-mapping>
